<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="BaseAppTheme">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:titleTextColor">#f8fcfb</item>
    </style>


    <style name="AlertDialogCustomDesign" parent="Theme.AppCompat.Dialog.Alert">
        <item name="android:textColor">#292929</item>
        <item name="android:textColorPrimary">#292929</item>
        <item name="android:fontFamily">sans-serif-condensed-medium</item>
        <item name="android:textSize">24sp</item>
        <item name="android:background">@color/dialogBackground</item>
        <item name="titleTextColor">#292929</item>
        <item name="android:textAlignment">center</item>
    </style>
</resources>